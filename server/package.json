{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "ts-node-dev --respawn --transpile-only index.ts",
		"start:debug": "ts-node-dev --inspect=4321 --respawn --transpile-only index.ts",
		"prod": "npm run typeorm:dropDB && ts-node --transpile-only index.ts",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"typeorm:dropDB": "npm run typeorm schema:drop",
		"lint": "eslint . --ext .ts",
		"update-webapi": "ts-node scripts/update_webapi.ts",
		"generate-seed": "ts-node node_modules/typeorm/cli.js migration:create -d src/seeders -n",
		"run-seeds": "ts-node node_modules/typeorm/cli.js migration:run",
		"revert-seeds": "ts-node node_modules/typeorm/cli.js migration:revert"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@elastic/elasticsearch": "^7.9.0",
		"@types/nodemailer": "^6.4.0",
		"aws-sdk": "^2.732.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"class-validator": "^0.12.2",
		"cors": "^2.8.5",
		"crypto-js": "^4.0.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"glob": "^7.1.6",
		"imagemin": "^7.0.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.11",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.3.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.25",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.7",
		"@types/glob": "^7.1.3",
		"@types/multer": "^1.4.3",
		"@types/node": "^14.0.26",
		"@types/passport-jwt": "^3.0.3",
		"@types/passport-local": "^1.0.33",
		"@types/uuid": "^8.0.0",
		"@typescript-eslint/eslint-plugin": "^3.6.1",
		"@typescript-eslint/parser": "^3.6.1",
		"eslint": "^7.2.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"ts-node-dev": "^1.0.0-pre.52",
		"typescript": "^3.9.7"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,md}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
